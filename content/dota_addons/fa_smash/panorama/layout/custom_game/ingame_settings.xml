		<Panel class="EntryPanel">
			<Label class="OnValue" id="OnAghanim" text="Aghanim:" />
			<Label class="Value" id="Aghanim" text="999" />	
		</Panel>
		<Panel class="EntryPanel">
			<Label class="OnValue" id="OnGoldMult" text="Gold Mult:" />
			<Label class="Value" id="GoldMult" text="999" />	
		</Panel>
		<Panel class="EntryPanel">
